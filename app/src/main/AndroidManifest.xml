<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.azaqo.sahabatbidan">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="android.app.default_searchable"
            android:value=".DataPasienResult" />

        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DataIbuatauAnak"
            android:label="Lihat Data" />
        <activity
            android:name=".ActivityPemeriksaan"
            android:label="@string/title_activity_pemeriksaan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DataPasiens"
            android:label="@string/title_activity_data_pasiens"
            android:parentActivityName=".DataIbuatauAnak"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.azaqo.sahabatbidan.DataIbuatauAnak" />
        </activity>
        <activity
            android:name=".DatadataKehamilan"
            android:label="@string/title_activity_datadata_kehamilan"
            android:parentActivityName=".DataPasiens"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.azaqo.sahabatbidan.DataPasiens" />
        </activity>
    </application>

</manifest>